    <param name="location" type="input" check="0"></param>   
    <running_script>python ../generators/pix2pixHD-master/image_control.py --checkpoints_dir ../models --name label2city --add_object person --feature ../generators/pix2pixHD-master/person.npy</running_script>
</transformation>

<transformation id="3">
    <name>Inject</name>
    <engine>Pix2pixHD</engine>
    <param name="object" type="fixed" check="1">bicycle</param>
    <param name="orientation" type="fixed" check="0">In the front</param>
    <param name="location" type="input" check="0"></param>   
    <running_script>python ../generators/pix2pixHD-master/image_control.py --checkpoints_dir ../models --name label2city --add_object bicycle --feature ../generators/pix2pixHD-master/bicycle.npy --feature_ex ../generators/pix2pixHD-master/rider.npy</running_script>
</transformation>

<transformation id="4">
    <name>Inject</name>
    <engine>OpenCV</engine>
    <param name="object" type="fixed" check="1">vehicle</param>
