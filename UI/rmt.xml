</transformation>

<transformation id="9">
    <name>ChangeScene</name>
    <engine>UGATIT</engine>
    <param name="weather" type="fixed" check="1">Day2rain</param>
    <running_script>python  ../generators/UGATIT/main.py --dataset day2rain --phase test --light True --img_size 224</running_script>
</transformation>

